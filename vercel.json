{
    "functions": {
        "api/*.js": {
            "maxDuration": 10
        }
    },
    "alias": [
        "nongaistudio.vercel.app"
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/public/login.html"
        },
        {
            "source": "/login",
            "destination": "/public/login.html"
        },
        {
            "source": "/signup",
            "destination": "/public/signup.html"
        },
        {
            "source": "/dashboard",
            "destination": "/public/dashboard.html"
        },
        {
            "source": "/app",
            "destination": "/public/studio.html"
        }
    ]
}